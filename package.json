{
  "name": "fetchguard",
  "version": "1.3.1",
  "description": "A secure fetch API library that runs in Web Workers with automatic token management",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./worker": {
      "import": "./dist/worker.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup && npm run typecheck",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "npm run test:node && npm run test:browser",
    "test:node": "vitest run --config vitest.config.ts",
    "test:watch": "vitest --config vitest.config.ts",
    "test:ui": "vitest --config vitest.config.ts --ui",
    "test:coverage": "vitest run --config vitest.config.ts --coverage",
    "test:browser": "npm run build && vitest --config vitest.browser.config.ts --browser chromium",
    "prepublishOnly": "npm run build",
    "prepack": "npm run build"
  },
  "keywords": [
    "fetch",
    "web-worker",
    "token",
    "authentication",
    "api",
    "proxy"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "ts-micro-result": "^2.1.10"
  },
  "devDependencies": {
    "@vitest/browser": "^4.0.2",
    "@vitest/browser-playwright": "^4.0.2",
    "@vitest/ui": "^4.0.2",
    "happy-dom": "^20.0.8",
    "playwright": "^1.45.1",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^4.0.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/minhtaimc/fetchguard.git"
  },
  "bugs": {
    "url": "https://github.com/minhtaimc/fetchguard/issues"
  },
  "homepage": "https://github.com/minhtaimc/fetchguard#readme"
}
